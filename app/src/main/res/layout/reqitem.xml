<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/admissioncard"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/img_btn_reject"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/decline"
        app:layout_constraintBottom_toBottomOf="@+id/imageView5"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.9" />

    <ImageButton
        android:id="@+id/img_btn_accept"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/accept"
        app:layout_constraintBottom_toTopOf="@+id/img_btn_reject"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.816"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="@+id/imageView5"
        app:layout_constraintVertical_bias="0.125" />

    <TextView

        android:id="@+id/tv_id"
        android:layout_width="165dp"
        android:layout_height="match_parent"
        android:layout_marginStart="30dp"
        android:fontFamily="@font/sf"
        android:text=""
        android:textColor="#05265E"
        android:textSize="17dp"

        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/img_btn_reject"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.467"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/img_btn_accept"
        app:layout_constraintVertical_bias="0.118" />

    <TextView
        android:id="@+id/tv_u_name"
        android:layout_width="165dp"
        android:layout_height="match_parent"
        android:fontFamily="@font/sf"
        android:text=""
        android:textColor="#05265E"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/img_btn_reject"
        app:layout_constraintEnd_toEndOf="@+id/imageView5"
        app:layout_constraintHorizontal_bias="0.573"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/tv_id"
        app:layout_constraintVertical_bias="0.144" />

    <TextView
        android:id="@+id/tv_mobnum"
        android:layout_width="165dp"
        android:layout_height="match_parent"
        android:fontFamily="@font/sf"
        android:text=""
        android:textColor="#05265E"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/img_btn_reject"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.609"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/tv_u_name"
        app:layout_constraintVertical_bias="0.277" />

    <TextView
        android:id="@+id/tv_alyear"
        android:layout_width="165dp"
        android:layout_height="match_parent"
        android:fontFamily="@font/sf"
        android:text=""
        android:textColor="#05265E"
        android:textSize="17dp"
        android:textStyle="bold"
        app:layout_constraintBottom_toTopOf="@+id/img_btn_reject"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@+id/imageView5"
        app:layout_constraintTop_toBottomOf="@+id/tv_mobnum"
        app:layout_constraintVertical_bias="0.62" />
</androidx.constraintlayout.widget.ConstraintLayout>